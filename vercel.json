{
  "crons": [
    {
      "path": "/api/cron/unsold-appointments",
      "schedule": "15 3 * * *"
    },
    {
      "path": "/api/integrations/sync",
      "schedule": "*/2 * * * *"
    },
    {
      "path": "/api/cron/send-review-followups",
      "schedule": "0 * * * *"
    },
    {
      "path": "/api/appointments/send-reminders",
      "schedule": "*/1 * * * *"
    }
  ],
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "soradin.com"
        }
      ],
      "destination": "https://www.soradin.com/:path*",
      "permanent": true
    }
  ]
}
